{
    "name": "filament/startupful-plugin",
    "description": "A Filament plugin for managing startups",
    "type": "library",
    "version": "0.1.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Filament\\Startupful\\": "src/"
        }
    },
    "authors": [
        {
            "name": "startupful",
            "email": "one@startupful.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.13.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\Startupful\\StartupfulServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}